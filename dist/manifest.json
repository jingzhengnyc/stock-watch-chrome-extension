{
  "name": "Real Time Stock Quote",
  "description": "Query the current stock price",
  "version": "0.0.1",
  "permissions": [
    "webRequest",
    "storage",
    "http://finance.google.com/",
    "https://www.google.com/finance/"
  ],
  "browser_action": {
      "default_title": "Real Time Stock Quote",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
